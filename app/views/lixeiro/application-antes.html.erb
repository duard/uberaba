<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) : Settings.env.appname %></title>
  <meta name="description" content="">
  <meta name="author" content="">
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>
  <%= render "sharedfront/menu" %>
  <div class="wrapper">
    <div class="container container_color">
      <%= render "sharedfront/messages" %>
      <%= yield %>
    </div>
  </div>
  <%= render "sharedfront/ofooter" %>
</body>
</html>
